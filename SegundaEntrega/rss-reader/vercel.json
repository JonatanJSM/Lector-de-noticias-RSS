{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "middlewares": [
              {
                "handler": "server"
              }
            ]
          }
      
    ]
  }


